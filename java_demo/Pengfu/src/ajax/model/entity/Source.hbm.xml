<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ajax.model.entity.Source" table="Source">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="url" type="java.lang.String">
			<column name="url"></column>
		</property>
		
		<property name="itype" type="java.lang.Integer">
			<column name="itype"></column>
		</property>
		
		<property name="rulestagid" type="java.lang.Integer">
			<column name="rulestagid"></column>
		</property>
		
		<property name="isGrab" type="java.lang.Boolean">
			<column name="isGrab"></column>
		</property>
		
		<property name="likes" type="java.lang.Integer">
			<column name="likes"></column>
		</property>
		
	</class>
	

	<class name="ajax.model.entity.ItemsRoll" table="itemsRoll">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="itemId" type="java.lang.Integer">
			<column name="itemId"></column>
		</property>
		
		<property name="rank" type="java.lang.Integer">
			<column name="rank"></column>
		</property>
		
		<property name="src" type="java.lang.String">
			<column name="src"></column>
		</property>
		
		<property name="title" type="java.lang.String">
			<column name="title"></column>
		</property>
		
		<property name="dateEntered" type="java.lang.String">
			<column name="dateEntered"></column>
		</property>
		
		<property name="deleted" type="java.lang.Boolean">
			<column name="deleted"></column>
		</property>
		
	</class>
	
	
	<class name="ajax.model.entity.Page" table="page">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="page" type="java.lang.Integer">
			<column name="page"></column>
		</property>

		<property name="items" type="java.lang.String">
			<column name="items"></column>
		</property>
		
	</class>	

	<class name="ajax.model.entity.NJoke" table="joke">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="jokeType" type="java.lang.Integer">
			<column name="jokeType"></column>
		</property>

		<property name="title" type="java.lang.String">
			<column name="title"></column>
		</property>
		
		<property name="content" type="java.lang.String">
			<column name="content"></column>
		</property>
		
	</class>
		

</hibernate-mapping>
