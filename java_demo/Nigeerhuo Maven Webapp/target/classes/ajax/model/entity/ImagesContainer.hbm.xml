<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ajax.model.entity.ImagesContainer" table="ImagesContainer">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="url" type="java.lang.String">
			<column name="url"></column>
		</property>
		
		<property name="diskPath" type="java.lang.String">
			<column name="diskPath"></column>
		</property>
		
		<property name="webPath" type="java.lang.String">
			<column name="webPath"></column>
		</property>
		
	</class>
	
</hibernate-mapping>
