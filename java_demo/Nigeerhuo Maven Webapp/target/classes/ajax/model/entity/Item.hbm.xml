<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ajax.model.entity.Item" table="ItemTest">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="url" type="java.lang.String">
			<column name="url"></column>
		</property>
		
		<property name="title" type="java.lang.String">
			<column name="title"></column>
		</property>
		
		<property name="summary" type="java.lang.String">
			<column name="summary"></column>
		</property>		
		
		<property name="content" type="java.lang.String">
			<column name="content"></column>
		</property>
				
		<property name="stamps" type="java.lang.String">
			<column name="stamps"></column>
		</property>	
		
		<property name="likes" type="java.lang.Integer">
			<column name="likes"></column>
		</property>		
		
		<property name="dislikes" type="java.lang.Integer">
			<column name="dislikes"></column>
		</property>		
		
		<property name="hasGetImage" type="java.lang.Boolean">
			<column name="hasGetImage"></column>
		</property>
		
		<property name="itype" type="java.lang.Integer">
			<column name="itype"></column>
		</property>		
				
		<property name="status" type="java.lang.Integer">
			<column name="status"></column>
		</property>
		
		<property name="username" type="java.lang.String">
			<column name="username"></column>
		</property>		
		
		<property name="userPersonalPageUrl" type="java.lang.String">
			<column name="userPersonalPageUrl"></column>
		</property>	
			
		<property name="backgroundInformation" type="java.lang.String">
			<column name="backgroundInformation"></column>
		</property>		
						
		<property name="dateEntered" type="java.lang.String">
			<column name="dateEntered"></column>
		</property>
				
		<property name="rulesTagId" type="java.lang.Integer">
			<column name="rulesTagId"></column>
		</property>
		
		<property name="previewImage" type="java.lang.String">
			<column name="previewImage"></column>
		</property>

		<property name="page" type="java.lang.Integer">
			<column name="page"></column>
		</property>
				
		<property name="hasImageUploadedToOss" type="java.lang.Boolean">
			<column name="hasImageUploadedToOss"></column>
		</property>
				
		<property name="statusForTest" type="java.lang.Integer">
			<column name="statusForTest"></column>
		</property>
				
	</class>
	


</hibernate-mapping>
