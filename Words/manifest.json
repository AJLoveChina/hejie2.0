{
    "manifest_version": 2,

    "name": "Words",
    "description": "AJ To The World.",
    "version": "2.0",

    "browser_action": {
        "default_icon": "/images/user.png",
        "default_popup": "/demo.html"
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/",
        "notifications",
        "activeTab"
    ],
    "content_scripts" : [
        {
            "matches": ["http://*/", "https://*/"],
            "js": ["/js/library/jquery.js", "/js/modules/fanyi/mouse.js"]
        }
    ]
}