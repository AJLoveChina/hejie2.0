<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ajax.model.Pic" table="Pic">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="url" type="java.lang.String">
			<column name="url"></column>
		</property>
		
		<property name="title" type="java.lang.String">
			<column name="title"></column>
		</property>
		
		<property name="description" type="java.lang.String">
			<column name="description"></column>
		</property>
		
		<property name="picUrl" type="java.lang.String">
			<column name="src"></column>
		</property>

		<property name="ctime" type="java.lang.String">
			<column name="dateEntered"></column>
		</property>


	</class>
	
		<class name="ajax.model.Config" table="Config">
		
		<id name="id" column="id" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		
		<property name="key" type="java.lang.String">
			<column name="akey"></column>
		</property>
		
		<property name="value" type="java.lang.String">
			<column name="avalue"></column>
		</property>
		
	</class>
</hibernate-mapping>
